configuration {
	default: 
		keyword = keyword.control;
		operation = keyword.operator.new;
	comment:
		line = "//";
		block:
			start = "/*";
			end = "*/";
}

rules {
	keyword:
		Entity = entity.name.class;
		Property = variable.language;
		SimpleType = entity.name.type;
	operation:
		Entity = storage.modifier;
}

